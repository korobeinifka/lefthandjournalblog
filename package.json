{
  "name": "blog",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "tinacms dev -c \"astro dev\"",
    "start": "astro dev",

    "prebuild": "node scripts/gen-favicons.mjs",

    "build:cms": "tinacms build",

    "build:admin-theme": "node scripts/patch-admin-head.mjs",

    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 npm run build:cms && astro check && astro build && node scripts/patch-admin-head.mjs",

    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@astrojs/check": "^0.4.1",
    "@astrojs/rss": "^4.0.4",
    "@astrojs/sitemap": "^3.0.5",
    "@astrojs/svelte": "^5.0.3",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/vercel": "^7.7.0",
    "astro": "^4.2.8",
    "astro-icon": "^1.0.4",
    "svelte": "^4.2.9",
    "tailwindcss": "^3.4.1",
    "tinacms": "2.9.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@iconify/svelte": "^3.1.6",
    "@tailwindcss/typography": "^0.5.13",
    "@tinacms/cli": "1.11.0",
    "cross-env": "^10.1.0",
    "favicons": "^7.2.0",
    "prettier": "^3.2.4",
    "prettier-plugin-astro": "^0.13.0"
  }
}
